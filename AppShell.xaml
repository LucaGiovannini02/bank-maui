<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="FinecoBankMobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:FinecoBankMobile.View"
    Shell.FlyoutBehavior="Flyout">
    <FlyoutItem>
        <ShellContent FlyoutItemIsVisible="False" ContentTemplate="{DataTemplate local:LoginPage}"/>
    </FlyoutItem>
    <FlyoutItem>
        <ShellContent FlyoutItemIsVisible="True" ContentTemplate="{DataTemplate local:HomePage}" Route="HomePage"/>
    </FlyoutItem>
    <FlyoutItem>
        <ShellContent ContentTemplate="{DataTemplate local:OperationsPage}" Route="OperationsPage"/>
    </FlyoutItem>
    <FlyoutItem>
        <ShellContent ContentTemplate="{DataTemplate local:RechargePage}" Route="RechargePage"/>
    </FlyoutItem>
    <FlyoutItem>
        <ShellContent ContentTemplate="{DataTemplate local:DetailPage}" Route="DetailPage"/>
    </FlyoutItem>
</Shell>
